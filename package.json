{
  "name": "angular-launchpad",
  "version": "0.0.16",
  "description": "Rapid Prototyping Backend with Angular Seed",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "clean": "rm -rf www && rm -rf coverage && rm -f *.pyc && rm -f launchpad/*.pyc && rm -f launchpad/*/*.pyc && rm -rf _trial_*",
    "distclean": "npm run clean && rm -rf node_modules && rm -rf pickle_jar",
    "prestart": "python -c \"import tornado\" || (echo 'Installing Requirements' && pip install tornado) && npm run build",
    "start": "./backend.py || backend.py",
    "prebuild": "node -e \"try { require.resolve('typescript') } catch (e) { console.log('Running npm install...'); require('child_process').execSync('npm install', {stdio: 'inherit'}); }\"",
    "build": "ng lint && ng build",
    "test": "ng lint && ng test --coverage",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.7",
    "@angular/cdk": "^16.1.6",
    "@angular/common": "^16.1.7",
    "@angular/compiler": "^16.1.7",
    "@angular/core": "^16.1.7",
    "@angular/forms": "^16.1.7",
    "@angular/localize": "^16.1.7",
    "@angular/material": "^16.1.6",
    "@angular/platform-browser": "^16.1.7",
    "@angular/platform-browser-dynamic": "^16.1.7",
    "@angular/platform-server": "^16.1.7",
    "@angular/router": "^16.1.7",
    "@angular/service-worker": "^16.1.7",
    "crypto-js": "^4.1.1",
    "rxjs": "^7.8.1",
    "ts-md5": "^1.3.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "^16.0.0",
    "@angular-devkit/build-angular": "^16.1.6",
    "@angular-eslint/builder": "^16.1.0",
    "@angular-eslint/eslint-plugin": "^16.1.0",
    "@angular-eslint/eslint-plugin-template": "^16.1.0",
    "@angular-eslint/schematics": "^16.1.0",
    "@angular-eslint/template-parser": "^16.1.0",
    "@angular/cli": "^16.1.6",
    "@angular/compiler-cli": "^16.1.7",
    "@angular/language-service": "^16.1.7",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.46.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
